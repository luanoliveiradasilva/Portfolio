name: dockercompose52290965190476384
services:
  app:
    build:
      context: D:\Documents\Projetos\Portfolio\app
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: App
